{
"manifest_version": 2,
"name": "Hey Beter",
"short_name": "Beter",
"description": "Changes the letter 'B' to the B emoji.",
"version": "1.1",

"browser_action": {
"default_icon": "src/icon19.png" },

"icons": { "16": "src/icon16.png",
"19": "src/icon19.png",
"48": "src/icon48.png",
"128": "src/icon128.png" },

"content_scripts": [{
"matches": [
"*://*/*"
],
"js": [
"content.js" ],
"run_at": "document_end" }]
}
